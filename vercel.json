{
    "version": 2,
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "functions": {
    "src/main.ts": {
      "memory": 800,
      "maxDuration": 10
    },
    "routes": [
      {
        "src": "/api-docs/(.*)",
        "dest": "src/main.ts"
      },
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE",
          "PATCH",
          "OPTIONS"
        ]
      }
    ]
  }
  